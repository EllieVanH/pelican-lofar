<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pelican>

<configuration version="1.0">
    <pipeline>
        <clients>
            <PelicanServerClient>
                <server host="127.0.0.1" port="2000" />
                <data type="JTestData" adapter="JTestDataAdapter" />
            </PelicanServerClient>
        </clients>

        <adapters>
            <JTestDataAdapter>
                <packet samples="256" />
            </JTestDataAdapter>
        </adapters>

        <modules>
            <JTestProc>
                <gain value="2.5"/>
            </JTestProc>
        </modules>

        <output>
            <streamers>
                <PelicanTCPBlobServer active="false">
                     <connection port="1234" />
                </PelicanTCPBlobServer>
                <JTestOutputStream name="precsv" active="true">
                     <file name="pre.csv" />
                </JTestOutputStream>
                <JTestOutputStream name="postcsv" active="true">
                     <file name="post.csv" />
                </JTestOutputStream>
            </streamers>
            <dataStreams>
                <stream name="all" listeners="PelicanTCPBlobServer" />
                <stream name="post" listeners="postcsv" />
                <stream name="pre" listeners="precsv" />
            </dataStreams>
        </output>

    </pipeline>
</configuration>

